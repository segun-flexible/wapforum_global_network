<div class="modal fade" id="createModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    style="display: none;" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <strong class="modal-title" id="exampleModalLabel">Create New Plan</strong>
                <a href="javascript:void(0)" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </a>
            </div>
            <form id="create">

                <div class="modal-body">

                    <div class="form-group">
                        <label for="title">Plan Title</label>
                        <input type="text" class="form-control" id="title" placeholder="E.g Diamond" required>
                        
                    
                    </div>
                   
                    
                    <div class="form-group">
                        <label for="price">Plan Price</label>
                        <input type="number" class="form-control" id="price" placeholder="5000" required>
                    </div>
                    
                    <div class="form-group">
                        <label for="activation">Plan Activation Fee</label>
                        <input type="number" class="form-control" id="activation" placeholder="1000" required>
                    </div>
                    
                    <div class="form-group">
                        <label for="roi">Plan ROI (%)</label>
                        <input type="number" class="form-control" id="roi" placeholder="Return On Investment Percentage" required>
                    </div>
                    
                    <div class="form-group">
                        <label for="referral">Plan Referral Bonus (%)</label>
                        <input type="number" class="form-control" id="referral" placeholder="Referral Bonus Percentage" required>
                    </div>
                    
                    <div class="form-group">
                        <label for="duration">Plan Duration (Days) </label>
                        <input type="number" class="form-control" id="duration" placeholder="Plan Duration" required>
                    </div>
                        
                

                    

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" id="button" class="btn btn-primary">Create</button>
                </div>
            </form>
        </div>
    </div>
</div>