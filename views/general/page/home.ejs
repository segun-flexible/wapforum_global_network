<%-include("../partials/header")%>

  

    <div class="content-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">


                    <!-- Featured Videos -->
                    <div class="content-block head-div">
                        <div class="cb-header">
                            <div class="row">
                                <div class="col-lg-10 col-sm-10 col-xs-8">
                                    <ul class="list-inline">
                                        <li>
                                            <a href="#" class="color-active">
                                                <span class="">All Videos</span>
                                                
                                            </a>
                                        </li>
                                        
                                    </ul>
                                </div>
                                
                            </div>
                        </div>
                        <div class="cb-content videolist">
                            <div class="row">
                                <%if(videos.length){%>
                                <%videos.map(vd =>{%>
                                <%-include("../partials/videoList",{vd})%>
                                <%})%>
                                <%}else{%>

                                <%}%>
                            </div>
                        </div>
                    </div>
                    <!-- /Featured Videos -->

                   

                    <!-- pagination -->
                    <div class="v-pagination">
                        <ul class="list-inline">
                            <%if(prevBtn){%>
                            <li class="v-pagination-prev"><a href="<%=link%>page=<%=prevBtn%>"><i class="cv cvicon-cv-previous"></i></a></li>
                            
                            <%}else{%>
                            <li class="v-pagination-prev"><a href="javascript:;"><i class="cv cvicon-cv-previous"></i></a></li>
                            <%}%>
                            
                            
                            <%if(nextBtn){%>
                            <li class="v-pagination-next"><a href="<%=link%>page=<%=nextBtn%>"><i class="cv cvicon-cv-next"></i></a></li>
                            <%}else{%>
                            <li class="v-pagination-next"><a href="javascript:;"><i class="cv cvicon-cv-next"></i></a></li>
                            <%}%>
                        </ul>
                    </div>
                    <!-- /pagination -->

                </div>
            </div>
        </div>
    </div>

<%-include("../partials/footer")%>